}



 
li.mm-mega-menu{
	position: static;
	.sidebar-slider{
		padding: 15px;
		.carousel {
			padding: 45px 0 0 0;
		}
	}
	
	&:hover{
		.mm-maga-main{
			.menu-hover-effect-open;
		}
		@media (max-width:991px){
			.mm-maga-main{
				.menu-hover-effect(0);
				display: none;
			}
		}
	}
	> a {
		cursor: pointer;
		.caret{
		@media (max-width:991px){
			display: block;
			width: 50px;
			height: 50px;
			float: right;
			position: absolute;
			right: 0;
			top: 0;
			background: darken(@as-theme, 5%); 
			border: none;
			&:after,
			&:before{
				width:25px;
				height: 1px;
				float: left;
				background:@as-light; 
				position: absolute;
				top: 0;
				left: 0;
				bottom: 0;
				right: 0;
				margin: auto;
				content: " ";
			}
			&:before{
				width:1px;
				height: 25px;
			}
		}
		}
		&.mob-menu-open{
				.caret{
					&:before{
						display:none;
					}
				}
			}
	}
	&.open-mob-menu{

		@media (max-width:991px){
			.mm-maga-main{
			    visibility: visible;
			    opacity: 1;
			    position: relative;
			    top: auto;
			    left: auto;
			    border: none;
			    box-shadow: none;
			    display: block;
			}
		}
	}
}
.mm-maga-main{
	.menu-hover-effect(0);
	.transition(all, 0.5s, ease);
	@media (max-width:991px){
		&.mm-mega-list {
			display: none;
		}
	}

	&.mm-mega-list{
		.mm-cat-title{
			width: 100%;
			display: inline-block;
			vertical-align: top;
			border-bottom: 1px solid darken(@as-light, 10%);
			padding: 15px 10px;
			margin: 0 0 10px 0;
			h2{
				margin: 0;
				padding: 0;
				font-size: @as-font-size + 4;
			}
		}

		.mmc-inner{
			width: 100%;
			display: inline-block;
